  stage('testing')
    node {
    echo "Testing the pipeline"
    }

  stage('checkout') {
      node {
        cleanWs()
        checkout scm
      }
  }


  stage('init') {
    node {  
      withCredentials([
        azureServicePrincipal('azserviceprincipal')]) {
